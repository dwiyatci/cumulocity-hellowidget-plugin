{
  "name": "cumulocity-hellowidget-plugin",
  "version": "1.0.0",
  "description": "A demo app for showcasing creation of a custom Cumulocity UI widget plugin",
  "keywords": [
    "cumulocity",
    "m2m",
    "examples",
    "demo",
    "boilerplate",
    "cumulocity ui plugin",
    "cumulocity custom widget"
  ],
  "homepage": "https://github.com/dwiyatci/cumulocity-hellowidget-plugin",
  "bugs": "https://github.com/dwiyatci/cumulocity-hellowidget-plugin/issues",
  "license": "MIT",
  "author": "Glenn Dwiyatcita <glenn.dwiyatcita@cumulocity.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/cumulocity-hellowidget-plugin.git"
  },
  "scripts": {
    "install:clean": "npm run clean && npm i",
    "clean": "rimraf node_modules",
    "start": "grunt server",
    "smartlyRegisterApp": "npm run reregisterApp || (npm run registerApp:noImports && npm run reregisterApp)",
    "reregisterApp": "npm run registerAllPlugin && npm run registerApp",
    "registerAllPlugin": "grunt pluginRegisterAll",
    "registerApp": "grunt appRegister",
    "registerApp:noImports": "grunt appRegister:noImports",
    "build": "rimraf build && grunt build"
  },
  "devDependencies": {
    "grunt": "0.4.5",
    "grunt-contrib-clean": "0.5.0",
    "grunt-contrib-connect": "0.7.1",
    "grunt-contrib-copy": "0.5.0",
    "grunt-contrib-cssmin": "0.9.0",
    "grunt-contrib-less": "0.11.1",
    "grunt-contrib-uglify": "0.4.0",
    "grunt-contrib-watch": "0.6.1",
    "grunt-angular-templates": "0.5.5",
    "grunt-cumulocity-ui-tasks": "1.1.4"
  }
}
